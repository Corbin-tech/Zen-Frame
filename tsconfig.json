{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "src/**/*", "components/**/*"],
  "exclude": ["dist", "node_modules"],
  "compilerOptions": {
    "noImplicitAny": false,
    "strictNullChecks": false,
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  }
}

